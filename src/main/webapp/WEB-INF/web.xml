<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>Saveserver</servlet-name>
  	<display-name>Saveserver</display-name>
  	<description></description>
  	<servlet-class>com.servlet.Saveserver</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Deleteserver</servlet-name>
  	<display-name>Deleteserver</display-name>
  	<description></description>
  	<servlet-class>com.servlet.Deleteserver</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Updateserver</servlet-name>
  	<display-name>Updateserver</display-name>
  	<description></description>
  	<servlet-class>com.servlet.Updateserver</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>Saveserver</servlet-name>
  	<url-pattern>/saveser</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Deleteserver</servlet-name>
  	<url-pattern>/delserv</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Updateserver</servlet-name>
  	<url-pattern>/updateserv</url-pattern>
  </servlet-mapping>
</web-app>
